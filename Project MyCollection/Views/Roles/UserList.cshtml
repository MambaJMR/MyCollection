@model IEnumerable<IdentityUser>
@using Microsoft.AspNetCore.Identity
<div class="container-fluid">
    <h2>@Locolize.GetKey("User List")</h2>
    <table class="table">
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>
                    <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">@Locolize.GetKey("Role")</a>
                </td>
            </tr>
        }
    </table>
</div>